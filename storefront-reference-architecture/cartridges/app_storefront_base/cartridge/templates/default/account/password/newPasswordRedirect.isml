<form id="newPasswordRedirect" action="${URLUtils.url('Account-DoSetNewPassword')}" method="POST">
    ${Resource.msg('msg.newpassword.redirect', 'account', null)}
    <button type="submit" name="continue" id="continue">
        ${Resource.msg('button.continue', 'account', null)}
    </button>
    <input type="hidden" name="token" id="token" value="${pdict.token}" />
</form>
<script> document.getElementById("continue").click(); </script>
