<isif condition="${price.type === 'range'}">
    <div class="product-price">
        <isinclude template="/product/components/pricing/range" />
    </div>
<iselse>
    <div class="product-price">
        <isset name="isLowPrice" value="${}" scope="page" />
        <isinclude template="/experience/components/commerce_assets/product/pricing/default" />
    </div>
</isif>
